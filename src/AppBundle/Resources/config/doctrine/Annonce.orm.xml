<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Annonce" table="annonces">
    <unique-constraints>
      <unique-constraint name="UNIQ_CB988C6FFF7747B4" columns="titre"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="string" column="titre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="image" type="string" column="image" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prix" type="decimal" column="prix" precision="2" scale="0" nullable="false"/>
    <field name="idUser" type="integer" column="id_user" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="false"/>
  </entity>
</doctrine-mapping>
